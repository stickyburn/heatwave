{
  "name": "Heatwave",
  "type": "dark",
  "colors": {
    "editor.background": "#1b1d26",
    "editor.foreground": "#ff007b",
    "activityBarBadge.background": "#1b1d26",
    "badge.background": "#1b1d26",
    "menu.background": "#1b1d26",
    "editorCursor.foreground": "#feff00",
    "terminalCursor.foreground": "#feff00",
    "breadcrumb.background": "#1b1d26",
    "input.background": "#1b1d26",
    "panel.background": "#1b1d26",
    "sideBar.background": "#1b1d26",
    "dropdown.background": "#1b1d26",
    "debugToolBar.background": "#1b1d26",
    "statusBar.background": "#1b1d26",
    "editorWidget.foreground": "#ff007b",
    "list.dropBackground": "#1b1d26",
    "editorPane.background": "#1b1d26",
    "selection.background": "#1b1d26",
    "terminal.background": "#1b1d26",
    "minimap.background": "#1b1d26",
    "titleBar.activeBackground": "#1b1d26",
    "titleBar.activeForeground": "#ff007b",
    "tab.inactiveBackground": "#1b1d26",
    "editor.foldBackground": "#1b1d26",
    "sideBar.dropBackground": "#1b1d26",
    "editorGroupHeader.tabsBackground": "#1b1d26",
    "sideBarSectionHeader.background": "#1b1d26",
    "activityBar.background": "#1b1d26",
    "searchEditor.findMatchBackground": "#1b1d26",
    "quickInput.background": "#1b1d26",
    "listFilterWidget.background": "#1b1d26",
    "editorSuggestWidget.background": "#1b1d26",
    "breadcrumbPicker.background": "#1b1d26",
    "quickInputTitle.background": "#1b1d26",
    "editorWidget.background": "#1b1d26",
    "editorHoverWidget.background": "#1b1d26",
    "activityBar.foreground": "#f20092",
    "activityBarBadge.foreground": "#feff00",
    "sideBar.foreground": "#feff00",
    "tab.activeForeground": "#feff00",
    "dropdown.listBackground": "#1b1d26",
    "sideBarTitle.foreground": "#ff007b",
    "settings.dropdownBackground": "#1b1d26",
    "menu.foreground": "#ff007b",
    "input.foreground": "#ff007b",
    "button.background": "#ff007b",
    "button.foreground": "#1b1d26",
    "statusBar.debuggingBackground": "#ff007b",
    "terminal.foreground": "#ff007b",
    "foreground": "#ff007b",
    "terminal.ansiYellow": "#a6b5fd",
    "terminal.ansiMagenta": "#ff007b",
    "input.border": "#ff007b",
    "focusBorder": "#ff007b",
    "editorLineNumber.foreground": "#ff007b",
    "statusBar.foreground": "#feff00",
    "scrollbarSlider.background": "#ff007b",
    "editorLink.activeForeground": "#feff00",
    "statusBar.noFolderBackground": "#1b1d26",
    "editorOverviewRuler.deletedForeground": "#ff007b"
  },
  "tokenColors": [
    {
        "name": "Comment color",
        "scope": [
            "comment",
            "markup.quote.markdown",
            "meta.diff",
            "meta.diff.header"
        ],
        "settings": {
            "foreground": "#6d6f73"
        }
    },
    {
        "name": "Text Color",
        "scope": [
            "meta.template.expression.js",
            "constant.name.attribute.tag.jade",
            "punctuation.definition.metadata.markdown",
            "punctuation.definition.string.end.markdown",
            "punctuation.definition.string.begin.markdown"
        ],
        "settings": {
            "foreground": "#D5CED9"
        }
    },
    {
        "name": "Cyan",
        "scope": [
            "variable",
            "support.variable",
            "entity.name.tag.yaml",
            "constant.character.entity.html",
            "source.css entity.name.tag.reference",
            "beginning.punctuation.definition.list.markdown",
            "source.css entity.other.attribute-name.parent-selector",
            "meta.structure.dictionary.json support.type.property-name"
        ],
        "settings": {
            "foreground": "#00e8c6"
        }
    },
    {
        "name": "Orange",
        "scope": [
            "markup.bold",
            "constant.numeric",
            "meta.group.regexp",
            "constant.other.php",
            "support.constant.ext.php",
            "constant.other.class.php",
            "support.constant.core.php",
            "fenced_code.block.language",
            "constant.other.caps.python",
            "entity.other.attribute-name",
            "support.type.exception.python",
            "source.css keyword.other.unit",
            "variable.other.object.property.js.jsx", "variable.other.object.js"
        ],
        "settings": {
            "foreground": "#f39c12"
        }
    },
    {
        "name": "Yellow",
        "scope": [
            "markup.list",
            "text.xml string",
            "entity.name.type",
            "support.function",
            "entity.other.attribute-name",
            "meta.at-rule.extend",
            "entity.name.function",
            "entity.other.inherited-class",
            "entity.other.keyframe-offset.css",
            "text.html.markdown string.quoted",
            "meta.function-call.generic.python",
            "meta.at-rule.extend support.constant",
            "entity.other.attribute-name.class.jade",
            "source.css entity.other.attribute-name",
            "text.xml punctuation.definition.string"

        ],
        "settings": {
            "foreground": "#FFE66D"
        }
    },
    {
        "name": "Pink",
        "scope": [
            "markup.heading",
            "variable.language.this.js",
            "variable.language.special.self.python"
        ],
        "settings": {
            "foreground": "#ff00aa"
        }
    },
    {
        "name": "Hot Pink",
        "scope": [
            "punctuation.definition.interpolation",
            "punctuation.section.embedded.end.php",
            "punctuation.section.embedded.end.ruby",
            "punctuation.section.embedded.begin.php",
            "punctuation.section.embedded.begin.ruby",
            "punctuation.definition.template-expression",
            "entity.name.tag"
        ],
        "settings": {
            "foreground": "#f92672"
        }
    },
    {
        "name": "Purple",
        "scope": [
            "storage",
            "keyword",
            "meta.link",
            "meta.image",
            "markup.italic",
            "source.js support.type"

        ],
        "settings": {
            "foreground": "#c74ded"
        }
    },
    {
        "name": "Blue",
        "scope": [
            "string.regexp",
            "markup.changed"
        ],
        "settings": {
            "foreground": "#7cb7ff"
        }
    },
    {
        "name": "Red",
        "scope": [
            "constant",
            "support.class",
            "keyword.operator",
            "support.constant",
            "text.html.markdown string",
            "source.css support.function",
            "source.php support.function",
            "support.function.magic.python",
            "entity.other.attribute-name.id",
            "markup.deleted"
        ],
        "settings": {
            "foreground": "#ee5d43"
        }
    },
    {
        "name": "Green",
        "scope": [
            "string",
            "text.html.php string",
            "markup.inline.raw",
            "markup.inserted",
            "punctuation.definition.string",
            "punctuation.definition.markdown",
            "text.html meta.embedded source.js string",
            "text.html.php punctuation.definition.string",
            "text.html meta.embedded source.js punctuation.definition.string",
            "text.html punctuation.definition.string",
            "text.html string"
        ],
        "settings": {
            "foreground": "#96E072"
        }
    },
    {
        "name": "Font Underline",
        "scope": [
            "entity.other.inherited-class"
        ],
        "settings": {
            "fontStyle": "underline"
        }
    }
]
}
